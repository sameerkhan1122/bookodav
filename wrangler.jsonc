{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "bookodav-worker",
  "main": "src/index.js",
  "compatibility_date": "2025-02-14",
  "account_id": "aa66288079176c3771c494ed33e7bcf3",
  "workers_dev": true,
  "observability": {
    "enabled": true
  },
  "r2_buckets": [
    {
      "binding": "MY_BUCKET",
      "bucket_name": "bookodav"
    }
  ],
  "placement": { "mode": "smart" },
  "assets": {
    "directory": "./src/public/",
    "binding": "ASSETS"
  }
}
